// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// FDA Nozzle Benchmark - Case 1 (Re = 500, Laminar)
// Kinematic pressure (p/rho)

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    wallInlet
    {
        type            zeroGradient;
    }
    
    wallCollector
    {
        type            zeroGradient;
    }
    
    wallThroat
    {
        type            zeroGradient;
    }
    
    wallOutlet
    {
        type            zeroGradient;
    }
    
    stepExpansion
    {
        type            zeroGradient;
    }
}

