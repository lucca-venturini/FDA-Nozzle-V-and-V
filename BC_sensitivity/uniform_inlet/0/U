FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// FDA Nozzle Benchmark - Case 1 (Re = 500, Laminar)
// BC Sensitivity Study: UNIFORM inlet profile (plug flow)
//
// U_avg = 0.04604 m/s (from Q = 5.20624e-6 m³/s, A = 1.131e-4 m²)
// Uniform velocity at all points on inlet face

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.04604);

boundaryField
{
    inlet
    {
        // Uniform velocity profile (plug flow)
        type            fixedValue;
        value           uniform (0 0 0.04604);
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    wallInlet
    {
        type            noSlip;
    }
    
    wallCollector
    {
        type            noSlip;
    }
    
    wallThroat
    {
        type            noSlip;
    }
    
    wallOutlet
    {
        type            noSlip;
    }
    
    stepExpansion
    {
        type            noSlip;
    }
}

// ************************************************************************* //
